{
  "hosting": {
    "headers": [{
      "source": "**/*.@(svg|css|jpeg|html)",
      "headers": [{"key": "Cache-Control", "value": "no-cache"}]}],
    "public": "public",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/templates/*"
    ],
    "rewrites": [
    ]
  }
}
